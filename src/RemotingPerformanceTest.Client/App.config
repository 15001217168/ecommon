<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
    <runtime>
        <gcServer enabled="true"/>
        <gcConcurrent enabled="true"/>
    </runtime>
  <appSettings>
    <add key="MessageCount" value="5000000"/>
    <add key="MessageSize" value="1024"/>
    <!-- Oneway, Sync, Async, Callback -->
    <add key="Mode" value="Oneway"/>
    <add key="ServerAddress" value=""/>
    <add key="ParallelThreadCount" value="4"/>
    <add key="SendMessageFlowControlWaitMilliseconds" value="5"/>
    <add key="SendMessageFlowControlCount" value="50000"/>
  </appSettings>
</configuration>
